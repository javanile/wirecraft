{
    "name": "javanile/wirecraft",
    "description": "Micro DSL language.",
    "version": "0.1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "francescobianco",
            "email": "bianco@javanile.org"
        }
    ],
    "require": {
        "php": "^7.2.9",
        "symfony/yaml": "^5.1",
        "gene-sis/lime": "*",
        "nette/tokenizer": "^3.1",
        "symfony/console": "^4.0|^5.0",
        "symfony/filesystem": "^4.0|^5.0",
        "symfony/process": "^4.2|^5.0",
        "webmozart/glob": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/propan"
    ],
    "autoload": {
        "psr-4": {
            "Javanile\\Wirecraft\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javanile\\Wirecraft\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
